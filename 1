-- This is the first query:

SELECT DISTINCT year from population_years;
-- Add your additional queries below:
SELECT *
FROM population_years
WHERE country = 'Gabon'
Order By population DESC;
  
select * from population_years where year = 2005 order by population asc limit 10;

select distinct country from population_years where year = 2010 and population > 100;

select distinct country from population_years where country like '%Islands%';

SELECT *
FROM population_years
WHERE country = 'Indonesia'and year=2000 or country = 'Indonesia' and year=2010;
